{
  "_tag": "Property of CKS © 2025 — Manifested by Freedom",
  "role": "contractor",
  "displayName": "Contractor Hub",
  "version": "1.0.0",
  "theme": {
    "primaryColor": "#10b981",
    "headerClass": "bg-emerald-600",
    "accentColor": "#059669"
  },
  "tabs": [
    {
      "id": "dashboard",
      "label": "Dashboard",
      "component": "Dashboard",
      "icon": "LayoutDashboard",
      "default": true,
      "requires": ["dashboard:view"]
    },
    {
      "id": "profile",
      "label": "My Profile",
      "component": "MyProfile",
      "icon": "User",
      "requires": ["profile:view"]
    },
    {
      "id": "ecosystem",
      "label": "My Ecosystem",
      "component": "Ecosystem",
      "icon": "Network",
      "requires": ["ecosystem:view"]
    },
    {
      "id": "services",
      "label": "My Services",
      "component": "MyServices",
      "icon": "Settings",
      "requires": ["services:manage"]
    },
    {
      "id": "orders",
      "label": "Orders",
      "component": "Orders",
      "icon": "ShoppingCart",
      "requires": ["orders:view"]
    },
    {
      "id": "reports",
      "label": "Reports",
      "component": "Reports",
      "icon": "FileText",
      "requires": ["reports:view"]
    },
    {
      "id": "support",
      "label": "Support",
      "component": "Support",
      "icon": "HelpCircle",
      "requires": ["support:access"]
    }
  ],
  "features": {
    "showRecentActions": true,
    "showBusinessMetrics": true,
    "allowServiceManagement": true,
    "allowOrderApprovals": true,
    "premiumSupport": true,
    "accountManagerAccess": true,
    "viewHierarchy": ["customer", "center", "crew"]
  },
  "api": {
    "baseUrl": "/api/contractor",
    "endpoints": {
      "profile": "/profile",
      "dashboard": "/dashboard",
      "activity": "/activity",
      "metrics": "/metrics",
      "customers": "/customers",
      "services": "/my-services",
      "ecosystem": "/ecosystem",
      "orders": "/orders",
      "requests": "/requests",
      "reports": "/reports",
      "feedback": "/feedback",
      "support": "/support",
      "capabilities": "/capabilities",
      "health": "/health"
    }
  },
  "permissions": {
    "default": ["dashboard:view", "profile:view"],
    "required": {
      "services:manage": "Manage service portfolio and selections",
      "ecosystem:view": "View business network and relationships",
      "orders:view": "View and manage service orders",
      "orders:approve": "Approve and deny service requests",
      "reports:view": "View business reports and feedback",
      "reports:manage": "Manage report status and responses",
      "support:access": "Access premium support services"
    }
  }
}