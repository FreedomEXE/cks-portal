{
  "_tag": "Property of CKS © 2025 — Manifested by Freedom",
  "role": "warehouse",
  "displayName": "Warehouse Hub",
  "version": "1.0.0",
  "theme": {
    "primaryColor": "#8b5cf6",
    "headerClass": "bg-violet-600",
    "accentColor": "#7c3aed"
  },
  "tabs": [
    {
      "id": "dashboard",
      "label": "Dashboard",
      "component": "Dashboard",
      "icon": "LayoutDashboard",
      "default": true,
      "requires": ["dashboard:view"]
    },
    {
      "id": "profile",
      "label": "My Profile", 
      "component": "MyProfile",
      "icon": "User",
      "requires": ["profile:view"]
    },
    {
      "id": "services",
      "label": "My Services",
      "component": "Services",
      "icon": "Settings",
      "requires": ["services:manage"]
    },
    {
      "id": "inventory",
      "label": "Inventory",
      "component": "Inventory", 
      "icon": "Package",
      "requires": ["inventory:manage"]
    },
    {
      "id": "orders",
      "label": "Orders",
      "component": "Orders",
      "icon": "ShoppingCart",
      "requires": ["orders:process"]
    },
    {
      "id": "deliveries",
      "label": "Deliveries",
      "component": "Deliveries",
      "icon": "Truck",
      "requires": ["delivery:track"]
    },
    {
      "id": "reports",
      "label": "Reports",
      "component": "Reports",
      "icon": "FileText",
      "requires": ["reports:generate"]
    },
    {
      "id": "support",
      "label": "Support",
      "component": "Support",
      "icon": "HelpCircle",
      "requires": ["support:access"]
    }
  ],
  "features": {
    "showInventoryMetrics": true,
    "showOrderTracking": true,
    "allowStockAdjustments": true,
    "deliveryIntegration": true,
    "viewHierarchy": ["manager"]
  },
  "api": {
    "baseUrl": "/api/warehouse",
    "endpoints": {
      "profile": "/profile",
      "dashboard": "/dashboard",
      "services": "/services", 
      "inventory": "/inventory",
      "orders": "/orders",
      "deliveries": "/deliveries",
      "support": "/support",
      "delivery": "/delivery",
      "reports": "/reports"
    }
  },
  "permissions": {
    "default": ["dashboard:view", "profile:view", "services:manage", "reports:generate"],
    "required": {
      "services:manage": "Manage warehouse services (delivery, equipment rental, repair)",
      "inventory:manage": "Manage inventory levels",
      "orders:process": "Process and fulfill orders",
      "delivery:track": "Track deliveries",
      "support:access": "Access support resources", 
      "reports:generate": "Generate warehouse reports"
    }
  }
}