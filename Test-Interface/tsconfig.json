{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
  "types": ["vite/client"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@cks-hubs/*": ["../Frontend/src/hubs/*"],
      "@cks-packages/*": ["../../packages/*"],
      "@cks-frontend/*": ["../Frontend/src/*"],
      "@cks-auth/*": ["../Auth/src/*"],
      "@clerk/clerk-react": ["./src/mocks/clerk-react"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}