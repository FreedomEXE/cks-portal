# Required by Vite - all vars must start with VITE_
VITE_API_URL=https://api.portal.ckscontracting.ca/api
VITE_APP_ENV=production
VITE_APP_VERSION=
VITE_GO_LIVE_DATE=

# Clerk (production instance)
VITE_CLERK_PUBLISHABLE_KEY=pk_live_your_key_here
VITE_CLERK_SIGN_IN_URL=/sign-in
VITE_CLERK_SIGN_UP_URL=/sign-up
VITE_CLERK_AFTER_SIGN_IN_URL=/hub
VITE_CLERK_AFTER_SIGN_UP_URL=/hub

# Dev auth is automatically disabled in production builds.
VITE_CKS_ENABLE_DEV_AUTH=false
